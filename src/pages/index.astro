---
import Header from "../components/Header.astro";
import Sidebar from "../components/Sidebar.astro";
---


<!DOCTYPE html>
<html
>
  <Header title="Kade Williams" />
  <body>
    <div class="content">
      <Sidebar selectedItem="home" />
      <nav class="main">
        <div class="container">
          <main class="fold-content-container">
            <div class="fold-content">
              <h1>Hi, I'm Kade!<br />Thanks for stopping by!</h1>
              <p class="lead"> 
                An experienced senior .NET full-stack and database developer specializing in building robust, internal business applications that automate critical processes and provide actionable insights across large-scale data environments.
                Deep Expertise in the .NET ecosystem (C#, .NET, SQL Server) to solve complex operational challenges with a database focus.
              </p>
            </div>
            <a href="mailto:kadewilliams0@gmail.com	" class="button w-button">Contact</a>
          </main>
          <div class="projects-section">
            <!-- Software Development -->
            <a href="/software-development" class="projects-container">
              <div class="single-projects-category">
                <div class="projects-category-image-container">
                  <img src="./assets/browser.svg" loading="lazy" alt="" class="projects-category-image" />
                </div>
              </div>
              <div class="single-projects-content">
                <h4>Software Development</h4>
                <div class="tech-tools-container">
                  <div class="mini-label-white gray-color">Technologies &amp; tools</div>
                  <div class="chips-container chips-container-small-width">
                    <div class="single-chip small-chip">.NET</div>
                    <div class="single-chip small-chip">C#</div>
                    <div class="single-chip small-chip">JavaScript</div>
                    <div class="single-chip small-chip">SQL Server</div>
                    <div class="single-chip small-chip">Node.js</div>
                    <div class="single-chip small-chip">Jenkins</div>
                  </div>
                </div>
              </div>
            </a>
            <!-- Personal Projects -->
            <a href="/personal-projects" class="projects-container">
              <div class="single-projects-category">
                <div class="projects-category-image-container devops-img">
                  <img src="./assets/favorite.svg" loading="lazy" alt="" class="projects-category-image" />
                </div>
              </div>
              <div class="single-projects-content">
                <h4>Personal Projects</h4>
                <div class="tech-tools-container">
                  <div class="mini-label-white gray-color">Technologies &amp; tools</div>
                  <div class="chips-container chips-container-small-width">
                    <div class="single-chip small-chip">.NET</div>
                    <div class="single-chip small-chip">C#</div>
                    <div class="single-chip small-chip">JavaScript</div>
                    <div class="single-chip small-chip">TypeScript</div>
                    <div class="single-chip small-chip">Node.js</div>
                    <div class="single-chip small-chip">Docker</div>
                    <div class="single-chip small-chip">React</div>
                    <div class="single-chip small-chip">Go</div>
                    <div class="single-chip small-chip">Postgres</div>
                  </div>
                </div>
              </div>
            </a>
          </div>

        </div>
      </nav>
    </div>

    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
  </body>
</html>