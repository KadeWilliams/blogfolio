---
import Header from "../components/Header.astro";
import Project from "../components/Project.astro";
import Sidebar from "../components/Sidebar.astro";
const tech = [".NET/C#", "JavaScript", "SQL Server", "PowerShell"]
const projects = [
  {
    title: "DBA Management Portal",
    problem: "DBAs couldn't reliably monitor or control 140+ servers and 800+ TB of data.",
    solution: "A centralized, web-based, internal management portal providing insights and control to every aspect of the SQL Server ecosystem.",
    keyDifference: {
       header: "Scale, Automation, Infrastructure.", 
       desc: [
        "Developed an automation engine that leverages PowerShell for reliable, cross-server communication and job execution.",
        "Delivered a centralized platform that provided the team with more granular control and better visibility than third-party tooling.",
        "Gained an understanding of the SQL Server architecture and the relationship between Servers, Instances, Databases and Availability Groups."
      ]
    },
  },
  {
    title: "Ministry Investment App",
    problem: "The executive team had no efficient way to track donations, partnerships, requests, and contributions, leading to manual errors, lack of visibility and slow processing.",
    solution: "A secure, reliable, internal application which centralized all financial data.",
    keyDifference: {
       header: "Data Sensitivity, Security, Executive Facing.", 
       desc: [
        "A full-stack application to replace a legacy process of manual spreadsheets and documents for tracking sensitive financial partnerships and contributions.",
        "The application provided the executive office with a single source of truth, automating reporting and ensuring data accuracy for critical decision making.",
        "Key responsibilities included end-to-end development with a focus on implementing robust user authentication using internal tooling, data validation and secure access controls to protect confidential information.",
      ]
    },
  },
  {
    title: "International Import Price Index (IPI) Processing",
    problem: "The aging legacy system used by the International group no longer fit the needs and the highly dynamic nature of processing imports, resulting in larger adjustments and corrections than necessary.",
    solution: "Building out a new all-in-one platform for the entire purchasing, verification and classification processes for increased usability, accuracy and control.",
    keyDifference: {
       header: "Modernize, Collaborate, Accuracy", 
       desc: [
        "Participated in development of the console, web, and API based applications associated with this process to run jobs against and communicate with other systems to remain in compliance with business objectives.",
        "Gained a high-level understanding of the entire PO process including correspondence with vendors and as well as the research to be done for tariff classification.",
      ]
    },
  },
];
