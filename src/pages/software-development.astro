---
<!DOCTYPE html>
<html>
  <Header title="Software Development | Kade Williams" />
  <body>
    <div class="content">
        <Sidebar selectedItem="software-development" />
      <nav class="main">
        <div class="container">
          <div class="back-nav-container">
            <a href="/" class="back-nav-button w-inline-block">
              <img src="/assets/back.svg" loading="lazy" alt="" class="back-nav-img" />
              <div>Back</div>
            </a>
          </div>
          <div class="content-container">
            <h1>Software Development</h1>
                <div class="mini-label-white">Technologies &amp; tools</div>
                <div class="chips-container">
                  {tech.map(techItem => <div class="single-chip">{techItem}</div>)}
                </div>
            <section class="items-container">
                <div class="resource-items">
                    <!-- {projects.map(project => (
                      <Project 
                        title={project.title} 
                      />
                    ))} -->
                    {projects.map(project => (
                      <Project 
                        title={project.title}
                        problem={project.problem}
                        solution={project.solution}
                        header={project.keyDifference.header}
                        desc={project.keyDifference.desc}
                      />
                    ))}
                  </div>
            </section>
          </div>
        </div>
      </nav>
    </div>

    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
  </body>
</html>
